{
    "name": "eshol",
    "version": "0.1.4",
    "description": "Electronic Ship Helicopter Operating Limits System",
    "main": "views/main.html",
    "chromium-args": "--touch-events",
    "window": {
        "title": "ESHOL",
        "icon": "link.png",
        "toolbar": false,
        "frame": false,
        "width": 1920,
        "height": 1080,
        "min_width": 400,
        "min_height": 400,
        "max_width": 1920,
        "max_height": 1080
    },
    "webkit": {
        "plugin": true
    },
    "repository": {
        "type": "git",
        "url": "git+github https://github.com/neoemptor/eshol.git",
        "path": "src/"
    },
    "author": "Darren Bailey <darren.bailey@dsbaileyfreelancer.com.au>",
    "license": "MIT",
    "homepage": "https://dsbaileyfreelancer.com.au",
    "scripts": {
        "tsc-build": "gulp",
        "dev": "nw",
        "prod": "nwbuild --platforms win32,win64,osx64,linux32,linux64 --buildDir dist/ ./"
    },
    "title": "eshol",
    "type": "simple",
    "dependencies": {
        "greensock": "^1.20.2",
        "path": "^0.12.7",
        "path-__dirname": "^2.1.0",
        "snapsvg": "^0.5.1",
        "url": "^0.11.0"
    },
    "devDependencies": {
        "jquery": "^3.3.1"
    },
    "build": {
        "nwVersion": "0.32.4"
    }
}
